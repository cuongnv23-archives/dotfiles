

! special
*.foreground:   #a0a0a0
*.background:   #232323
*.cursorColor:  #a0a0a0
! black
*.color0:       #303030
*.color8:       #686868
! red
*.color1:       #d370a3
*.color9:       #ffa7da
! green
*.color2:       #6d9e3f
*.color10:      #a3d572
! yellow
*.color3:       #b58858
*.color11:      #efbd8b
! blue
*.color4:       #6095c5
*.color12:      #98cbfe
! magenta
*.color5:       #ac7bde
*.color13:      #e5b0ff
! cyan
*.color6:       #3ba275
*.color14:      #75daa9
! white
*.color7:       #cfcfcf
*.color15:      #ffffff


Xft.antialias		: true
Xft.autohint		: true
Xft.dpi			: 135
Xft.hinting		: true
Xft.hintstyle		: hintfull
Xft.rgba			: rgb
*font: xft:Droid Sans Mono:pixelsize=12
URxv*perl-ext-common:default,clipboard
URxvt*perl-lib: /usr/lib/urxvt/perl/
URxvt*scrollBar: false
URxvt*scrollColor: #404040
URxvt*depth: 32
URxvt*background: [97]#222222
URxvt*url-select.launcher: /bin/firefox
URxvt*underlineURLs: True
URxvt*url-select.button: 1
URxvt*keysym.M-u: perl:url-select:select_next
URxvt.keysym.Shift-Up: command:\033]720;1\007

! Transparent

URxvt.transparent		: false
URxvt.shading			  : 80

! Setting copy/paste
